		"svelte": "^4.2.16",
		"svelte-check": "^3.7.1",
		"tailwindcss": "3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unplugin-icons": "0.19.0",
		"uuid": "9.0.1",
		"validator": "13.12.0",
		"vite": "^5.2.11",
