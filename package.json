		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@types/node": "20.12.11",
		"@types/uuid": "9.0.8",
		"@types/validator": "13.11.9",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
