		"svelte": "^5.11.2",
		"svelte-check": "^4.1.1",
		"tailwindcss": "3.4.16",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"unplugin-icons": "0.21.0",
		"uuid": "11.0.3",
		"validator": "13.12.0",
		"vite": "^5.2.11",
		"vite-plugin-tailwind-purgecss": "0.3.5"
